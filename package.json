{
  "name": "revamp-frontend",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0",
    "deploy": "websync --invalidateDeletes --yes",
    "build": "ng build --prod --aot",
    "build.stg": "ng build --configuration staging",
    "build.prod": "ng build --configuration production",
    "prebuild": "npm rebuild node-sass",
    "precache": "sw-precache --verbose --config=sw-precache-config.js",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "start.http": "ng build --prod && http-server ./dist -o -p 4101",
    "serve.http": "http-server ./dist -o -p 4101",
    "start.https": "ng build --prod && http-server ./dist -S -o -p 4102",
    "serve.https": "http-server ./dist -S -o -p 4102",
    "createkey": "openssl req -x509 -newkey rsa:4096 -keyout key.pem -out cert.pem -nodes -days 365 -subj '/CN=localhost'"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.1.3",
    "@angular/cdk": "~7.3.2",
    "@angular/common": "^7.1.3",
    "@angular/compiler": "^7.1.3",
    "@angular/core": "^7.1.3",
    "@angular/forms": "^7.1.3",
    "@angular/http": "^7.1.3",
    "@angular/material": "^7.3.2",
    "@angular/platform-browser": "^7.1.3",
    "@angular/platform-browser-dynamic": "^7.1.3",
    "@angular/platform-server": "^7.1.3",
    "@angular/pwa": "^0.13.9",
    "@angular/router": "^7.1.3",
    "@angular/service-worker": "^7.1.3",
    "@auth0/angular-jwt": "^2.1.0",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "animate.css": "^3.7.1",
    "ngx-device-detector": "^1.3.5",
    "ngx-wow": "^2.0.0",
    "reset-css": "^4.0.1",
    "rxjs": "^6.3.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.13.6",
    "@angular/cli": "7.1.3",
    "@angular/compiler-cli": "^7.2.9",
    "@types/jasmine": "3.3.2",
    "@types/node": "~10.12.15",
    "codelyzer": "^4.5.0",
    "jasmine-core": "~3.3.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.4",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~2.0.0",
    "karma-coverage-istanbul-reporter": "^2.0.4",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "protractor": "~5.4.1",
    "rxjs-compat": "^6.3.3",
    "ts-node": "~7.0.1",
    "tslint": "^5.11.0",
    "typescript": "3.1.6",
    "websync": "0.0.13"
  }
}
